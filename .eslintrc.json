{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": [
		"wikimedia/client/es6",
		"wikimedia/mediawiki",
		"wikimedia/jquery"
	],
	"globals": {
		"MoreMenu": "writable",
		"process": "readonly"
	},
	"parserOptions": {
		"ecmaVersion": 9
	},
	"rules": {
		"es-x/no-rest-spread-properties": "off",
		"mediawiki/class-doc": "off",
		"no-bitwise": "off",
		"no-jquery/no-global-selector": "off"
	}
}
